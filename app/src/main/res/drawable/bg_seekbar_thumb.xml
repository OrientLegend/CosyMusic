<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!--点击状态-->
    <item android:drawable="@drawable/seekbar_thumb_btn" android:state_pressed="true"/>

    <!--焦点状态-->
    <item android:drawable="@drawable/seekbar_thumb_btn" android:state_focused="true"/>

    <!--选择状态-->
    <item android:drawable="@drawable/seekbar_thumb_btn" android:state_selected="true"/>

    <!--默认状态-->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/black"/>
            <size android:height="12dp"
                android:width="12dp"/>
            <corners android:radius="6dp"/>
        </shape>

    </item>

</selector>